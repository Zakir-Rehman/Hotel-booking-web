<%- layout("/layouts/boilerplate") -%>
  <!-- Gallery -->
 <script>
    let geo = <%- JSON.stringify(listing.geometry || {}) %>;
</script>

  <section class="gallery">
    <div class="main-img">
      <img src="<%=listing.image.url %>" alt="Main Image" />
    </div>
  </section>

  <div class="main-div">

    <!-- Main Content -->
    <main class="show-container">
      <div class="menu-listing">
        <ol>
          <a href="#about" class="nav-link">
            <li>About</li>
          </a>
          <a href="#location" class="nav-link ">
            <li>Location</li>
          </a>
          <a href="#reviews" class="nav-link ">
            <li>Reviews</li>
          </a>
          <a href="#comments" class="nav-link ">
            <li>Comments</li>
          </a>
          <a href="#options" class="nav-link ">
            <li>Options</li>
          </a>
        </ol>
      </div>
      <!-- <div class="menu-listing">
        <ol>
          <li><a href="#about" class="nav-link active">About</a></li>
          <li><a href="#location" class="nav-link">Location</a></li>
          <li><a href="#reviews" class="nav-link">Reviews</a></li>
          <li><a href="#comments" class="nav-link">Comments</a></li>
          <li><a href="#availability" class="nav-link">Availability</a></li>
        </ol>
      </div> -->


